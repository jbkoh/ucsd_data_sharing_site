#Adjust all settings that start with TK to customise this theme to your site.  Also remove the TK
theme = "martial-paper"
baseUrl = "http://mesl-exp.ucsd.edu:1713"
languageCode = "en-us"
title = "UCSD Building Data"

[params]
  aboutpage = "about" #This makes the "about" link in the header function.  It must be the exact filename of your about page without the file extension.  E.g. if your about page is called "mysiteabout.md" then enter in "mysiteabout"
  author = "Peter Carleton" #Goes in the meta info of your webpage to help identify you as the author of the content
  copyrightstatement = "TODO." #Will appear in the footer of each page.
  description = "The website sharing data of buildings in UCSD." #Goes in the meta info of your webpage to describe the content of your site
  siteclass = "Sharing" #Optional.  If you set it, it will display a subtitle in a contrasting color (adjustable within the CSS) on the site's index page.
  tagline = "" #Optional.  If you set it, it will display a tagline for the website underneath the titles.  By default it appears italicized.
